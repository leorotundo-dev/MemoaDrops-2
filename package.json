    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@sentry/node": "^10.24.0",
    "@sentry/profiling-node": "^10.24.0",
    "@types/puppeteer": "^5.4.7",
    "adm-zip": "^0.5.16",
    "apn": "^2.2.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.7.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.1.0",
    "google-auth-library": "^9.15.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "knex": "^3.1.0",
    "mammoth": "^1.11.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.52.1",
    "pdf-parse": "^1.1.4",
    "pg": "^8.11.5",
    "pgvector": "^0.1.8",
