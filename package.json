{
  "name": "memodrops-v3",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx src/server.ts",
    "worker": "tsx src/worker.ts",
    "build": "tsc -p tsconfig.json && mkdir -p dist/db/migrations && cp src/db/migrations/*.sql dist/db/migrations/",
    "start": "node --enable-source-maps dist/server.js",
    "start:worker": "node --enable-source-maps dist/worker.js",
    "migrate": "ts-node src/db/run-migration.ts",
    "lint": "echo 'add your linter here'",
    "test": "jest --config jest.config.ts --runInBand",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:ci": "jest --config jest.config.ts --coverage"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.1",
